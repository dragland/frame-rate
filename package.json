{
  "name": "frame-rate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:redis": "npm run redis:start && REDIS_URL=redis://localhost:6379 npm run dev",
    "redis:start": "docker run -d --name frame-rate-redis -p 6379:6379 redis:alpine || docker start frame-rate-redis",
    "redis:stop": "docker stop frame-rate-redis",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "ioredis": "^5.3.2",
    "next": "^14.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "happy-dom": "^20.0.11",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "vitest": "^4.0.16"
  }
}
